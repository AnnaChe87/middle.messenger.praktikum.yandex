<div class="chat-header">
  <div class="chat-header-avatar"></div>
  <h4 class="chat-header-title">{{title}}</h4>
  <div class="chat-header-actions"></div>
</div>
<div class="chat-content">
  {{#each messages}}
    {{{this}}}
  {{/each}}
</div>
<div class="chat-footer">
  <div class="chat-footer-attach">
    {{{attach}}}
  </div>
  <form class="chat-footer-form">
    {{{input}}}
    {{{btn}}}
  </form>
</div>